      "cell_type": "code",
      "source": [
        "def find_total_events(shared_df_row):\n",
        "  total_likes = users_df[(users_df['contentId']==shared_df_row[\"contentId\"]) & (users_df[\"eventType\"]==\"LIKE\")].shape[0]\n",
        "  total_views = users_df[(users_df['contentId']==shared_df_row[\"contentId\"]) & (users_df[\"eventType\"]==\"VIEW\")].shape[0]\n",
        "  total_bookmarks = users_df[(users_df['contentId']==shared_df_row[\"contentId\"]) & (users_df[\"eventType\"]==\"BOOKMARK\")].shape[0]\n",
        "  total_follows = users_df[(users_df['contentId']==shared_df_row[\"contentId\"]) & (users_df[\"eventType\"]==\"FOLLOW\")].shape[0]\n",
        "  total_comments = users_df[(users_df['contentId']==shared_df_row[\"contentId\"]) & (users_df[\"eventType\"]==\"COMMENT CREATED\")].shape[0]\n",
        "\n",
        "  return total_likes + total_views + total_bookmarks + total_follows + total_comments\n",
        "shared_df[\"total_events\"] = shared_df.apply(find_total_events, axis=  1)\n",
        "shared_df.head()"
      ],
      "metadata": {
        "colab": {
